<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@this.ViewData["Title"] - BeautyBooking</title>

        <link href="~/Content/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/plugins/OwlCarousel2-2.2.1/owl.carousel.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/plugins/OwlCarousel2-2.2.1/owl.theme.default.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/plugins/OwlCarousel2-2.2.1/animate.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/css/site.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/css/responsive.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/Content/css/sidebar.css" rel="stylesheet" asp-append-version="true" />
 
    @this.RenderSection("Styles", required: false)
</head>
<body>

    <div class="super_container">

        <!-- Menu -->
        @Html.Partial("_ManagerMenuPartial")

        <!-- Home -->
        @Html.Partial("_ManagerHeaderPartial")

        <div style="padding-top: 90px;">

            <div class="text-center">
                <a style="color: #BA55D3" href="/Home/Index"><strong>BACK TO HOME PAGE</strong></a>
            </div>
            <hr class="bg-dark mx-4" style="height: 6px;" />
            <div class="d-flex" id="wrapper">

                <!-- Sidebar -->
                @Html.Partial("_ManagerSidebarPartial")

                <!-- Page Content -->
                <div id="page-content-wrapper">
                    <div class="container-fluid">
                        <div class="col-12">
                            <main role="main">
                                @this.RenderBody()
                            </main>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <hr class="bg-dark mx-4" style="height: 6px;" />

        <!-- Footer -->
        @Html.Partial("_FooterPartial")
    </div>

        <script src="~/Content/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
        <script src="~/Content/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
        <script src="~/Content/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
        <script src="~/Content/js/popper.min.js" asp-append-version="true"></script>
        <script src="~/Content/lib/bootstrap/dist/js/bootstrap.min.js" asp-append-version="true"></script>
        <script src="~/Content/plugins/OwlCarousel2-2.2.1/owl.carousel.js" asp-append-version="true"></script>
        <script src="~/Content/plugins/easing/easing.js" asp-append-version="true"></script>
        <script src="~/Content/plugins/parallax-js-master/parallax.min.js" asp-append-version="true"></script>
        <script src="~/Content/js/site.min.js" asp-append-version="true"></script>

    @this.RenderSection("Scripts", required: false)
</body>
</html>
